<%- if controller_name != 'sessions' %>
  <%= link_to "既に登録済みの方はこちら", new_session_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "新規登録の方はこちら", new_registration_path(resource_name) %><br />
  <%= link_to "管理者の方はこちら", new_admin_session_path %><br />
<% end %>